{
    "name": "Allocation",
    "dockerComposeFile": "docker-compose.yml",
    "service": "allocation",
    "workspaceFolder": "/workspace",
    "postCreateCommand": "pip install -r requirements.txt",
    "shutdownAction": "stopCompose",
    "forwardPorts": [
        5432,
        5005
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python"
            ]
        }
    }
}
